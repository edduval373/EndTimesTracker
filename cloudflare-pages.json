{
  "build": {
    "command": "npm run build",
    "destination": "dist/public"
  },
  "env": {
    "NODE_VERSION": "20",
    "NPM_VERSION": "10"
  },
  "functions": {
    "directory": "functions"
  },
  "redirects": [
    {
      "source": "/*",
      "destination": "/index.html",
      "status": 200
    }
  ]
}