{
  "build": {
    "command": "npm run build",
    "destination": "dist/public"
  },
  "env": {
    "NODE_VERSION": "20",
    "NPM_VERSION": "10"
  },
  "redirects": [
    {
      "source": "/api/*",
      "destination": "https://endtimestracker.app/api/:splat",
      "status": 200
    },
    {
      "source": "/*",
      "destination": "/index.html",
      "status": 200
    }
  ]
}